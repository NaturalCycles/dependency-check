{
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "version": "0.0.10",
  "name": "@naturalcycles/dependency-check",
  "description": "NPM and JAVA dependency analyser",
  "author": "Natural Cycles Team",
  "license": "MIT",
  "bin": {
    "nc-depcheck": "dist/index.js"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p .",
    "build-prod": "build-prod-esm-cjs",
    "tsc": "npx tsc",
    "watch": "npx tsc -w"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "dist-esm",
    "src",
    "!src/test",
    "!src/**/*.test.*",
    "!src/**/__snapshots__"
  ],
  "exports": {
    "import": "./dist-esm/index.js",
    "require": "./dist/index.js"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@naturalcycles/nodejs-lib": "^12.12.2",
    "commonmark": "^0.30.0",
    "depcheck": "^1.4.0",
    "fs": "0.0.1-security",
    "got": "^10.7.0",
    "hosted-git-info": "^4.0.2",
    "license-checker": "^25.0.1",
    "path": "^0.12.7",
    "pdfkit": "^0.12.3",
    "pdfkit-commonmark": "^1.1.3",
    "pom-parser": "^1.2.0",
    "xml2json": "^0.12.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@naturalcycles/dev-lib": "^12.10.0",
    "@types/commonmark": "^0.27.5",
    "@types/hosted-git-info": "^3.0.1",
    "@types/pdfkit": "^0.12.0",
    "@types/yargs": "^16.0.1",
    "patch-package": "^6.4.7",
    "typescript": "^4.2.4"
  }
}
